{
    "name": "rowbot/url",
    "description": "A WHATWG URL spec compliant URL parser for working with URLs and their query strings.",
    "keywords": ["url", "url-parser", "uri", "querystring", "query-string", "url-parsing", "whatwg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Trevor Rowbotham",
            "homepage": "http://trowbotham.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-gmp": "*",
        "ext-intl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "php-coveralls/php-coveralls" : "^2.1",
        "phpunit/phpunit": "^7.0",
        "phpstan/phpstan": "^0.10.1",
        "phpstan/phpstan-strict-rules": "^0.10.0",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpstan/phpstan-deprecation-rules": "^0.10.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Rowbot\\URL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rowbot\\URL\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse -c phpstan-ci.neon -l max src",
        "phpunit": "phpunit --coverage-text",
        "test": [
            "@phpunit",
            "@phpstan"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
