{
    "name": "rowbot/url",
    "description": "A WHATWG URL spec compliant URL parser for working with URLs and their query strings.",
    "keywords": ["url", "url-parser", "uri", "querystring", "query-string", "url-parsing", "whatwg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Trevor Rowbotham",
            "homepage": "http://trowbotham.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-gmp": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "lib-ICU": ">=4.6"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^7.0 || ^8.0",
        "symfony/cache": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Rowbot\\URL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rowbot\\URL\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "phpunit --coverage-text",
        "test": [
            "@phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
